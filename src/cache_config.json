{
    "cache": {
        "state": 1,
		"driver": "filesystem",
        "dynamic": 0,
        "print": 0,
        "cache_lifetime": 360,
        "cpu": 80,
        "memory": 80,
        "vendor": "cache.cache",
        "adapter": "pllano.cache",
        "clear": 0,
        "test": {
            "pool": "\\Pllano\\Caching\\Cache\\TestCache",
            "path": "cache\/_test_cache"
        },
        "memcached": {
            "pool": "\\Cache\\Adapter\\Memcached\\MemcachedCachePool",
            "host": "127.0.0.1",
            "port": "11211"
        },
        "memcache": {
            "pool": "\\Cache\\Adapter\\Memcache\\MemcacheCachePool",
            "host": "127.0.0.1",
            "port": "11211"
        },
        "filesystem": {
            "pool": "\\Cache\\Adapter\\Filesystem\\FilesystemCachePool",
            "filesystem_adapter": "\\League\\Flysystem\\Adapter\\Local",
            "filesystem": "\\League\\Flysystem\\Filesystem",
            "path": "cache\/_file_cache"
        },
        "json": {
            "pool": "\\Pllano\\Caching\\Cache\\JsonCache",
            "path": "cache\/_json_cache"
        },
        "elasticsearch": {
            "pool": "\\ApiShop\\Adapter\\Cache\\Elasticsearch",
