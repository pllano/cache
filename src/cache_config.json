            "host": "127.0.0.1",
            "port": "11211"
        },
        "array": {
            "pool": "\\Cache\\Adapter\\PHPArray\\ArrayCachePool"
        },
        "apcu": {
            "pool": "\\Cache\\Adapter\\Apcu\\ApcuCachePool"
        },
        "apc": {
            "pool": "\\Cache\\Adapter\\Apc\\ApcCachePool"
        },
        "void": {
            "pool": "\\Cache\\Adapter\\Void\\VoidCachePool"
        },
        "illuminate": {
